        <jsScript_script>// FunÃ§Ã£o robusta pra detectar e padronizar datas para dd/MM/yyyy
function converterDataRobusta(valor) {
  if (valor == null) return null;

  // limpa espaÃ§os e aspas externas
  var v = valor.toString().trim();
  if (v.length == 0) return null;
  v = v.replace(/^"+|"+$/g, ''); // remove aspas externas
  v = v.replace(/\s+/g, ''); // remove espaÃ§os internos desnecessÃ¡rios

  // ðŸ”¹ remove qualquer parte de hora, como 00:00:00 ou 000000 no final
  v = v.replace(/T?\d{2}:\d{2}(:\d{2})?$/, ''); // remove HH:MM:SS
  v = v.replace(/0{6}$/, ''); // remove 000000 no final (hora sem separador)
  v = v.replace(/-00$/, ''); // remove casos tipo 2021-01-01-00

  // ðŸ”¹ remove traÃ§os, barras ou pontos duplicados no final
  v = v.replace(/[-\/\.]+$/, '');

  var d = null;

  // 1) somente dÃ­gitos (ex: 20210101 ou 01012021)
  if (/^\d{8}$/.test(v)) {
    var first4 = parseInt(v.substring(0,4), 10);
    var last4 = parseInt(v.substring(4,8), 10);
    // se os primeiros 4 parecem ser ano plausÃ­vel -> yyyyMMdd
    if (first4 >= 1900 &amp;&amp; first4 &lt;= 2100) {
      try { d = str2date(v, "yyyyMMdd"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
    }
    // se os Ãºltimos 4 parecem ser ano plausÃ­vel -> ddMMyyyy
    if (last4 >= 1900 &amp;&amp; last4 &lt;= 2100) {
      try { d = str2date(v, "ddMMyyyy"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
    }
    // fallback: tenta ambos
    try { d = str2date(v, "yyyyMMdd"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
    try { d = str2date(v, "ddMMyyyy"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
    return null;
  }

  // 2) formatos com traÃ§o ou barra (ex: 2021-01-01, 01/01/2021, etc.)
  try { d = str2date(v, "yyyy-MM-dd"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
  try { d = str2date(v, "dd/MM/yyyy"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
  try { d = str2date(v, "yyyy/MM/dd"); return date2str(d, "dd/MM/yyyy"); } catch(e){}
  try { d = str2date(v, "dd-MM-yyyy"); return date2str(d, "dd/MM/yyyy"); } catch(e){}

  // se nada der certo
  return null;
}

// ðŸ”¹ Substitui o prÃ³prio campo (sem criar novo)
datainfracao = converterDataRobusta(datainfracao);

// se tiver outra data, repita:
// dataimplantacao = converterDataRobusta(dataimplantacao);
</jsScript_script>
